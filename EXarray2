package Array;
import java.util.Scanner;

public class ArrayPrincipal {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        System.out.printf("Quantos valores deseja digitar? ");
        int qntdArray = input.nextInt();
        double arrayNum[]= new double[qntdArray];
        double somaArray = 0;

        for (int i = 0; i < arrayNum.length; i++) {
            System.out.print("Digite um nÃºmero: ");
            arrayNum[i] = input.nextDouble();
            somaArray = somaArray + arrayNum[i];

        }
        System.out.println(somaArray);


    }
}



